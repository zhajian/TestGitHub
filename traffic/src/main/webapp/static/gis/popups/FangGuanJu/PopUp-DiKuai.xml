<!-- 地块 -->
<?xml version="1.0" ?>
<configuration>
	<title>{LandName}</title>
	<fields>
		<field name="LandName" />
		<field name="LandPlace" alias="位置：" visible="true" />
		<field name="LandArea" alias="面积：" visible="true" />
		<field name="LandType" alias="土地类型：" visible="true" />
		<field name="LandYear" alias="年份：" visible="true" />
	</fields>
	<showzoomtobutton>false</showzoomtobutton>
	<buttons>
		<button label="详细信息" action="openDevice" type="CJQ" idfield="ID" />
	</buttons>
</configuration>
