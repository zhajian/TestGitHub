<!--标线 -->
<configuration>
	<layer>http://{serverip}/arcgis/rest/services/TianJin/tianjin_device/MapServer/6</layer>
	
	<refreshrate>0</refreshrate>
	<refreshneedquery>false</refreshneedquery>
	
	<widgetvisible>false</widgetvisible>
	
	<clickfunction>
		<action>showInfoWindow</action>
	</clickfunction>
	
	<popup>
		<title>{ITEMCODE}</title>
		<fields>
			<field name="ITEMCODE" />
			<field name="PICTURE"/>
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<popupskinclass>DeviceWithBlob</popupskinclass>
	
	<simplerenderer>
		<simplemarkersymbol color="0xff830f" size="10"/>
	</simplerenderer>
</configuration>