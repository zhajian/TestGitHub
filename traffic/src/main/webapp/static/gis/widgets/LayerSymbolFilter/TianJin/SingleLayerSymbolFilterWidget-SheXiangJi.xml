<!--摄像机 -->
<configuration>
	<layer>http://{serverip}/arcgis/rest/services/TianJin/tianjin_device/MapServer/2</layer>
	<layername>摄像机</layername>
	
	<refreshrate>0</refreshrate>
	<refreshneedquery>false</refreshneedquery>
	
	<widgetvisible>false</widgetvisible>
	
	<clickfunction>
		<action>OpenDevice</action>
		<type>SheXiangJi</type>
		<idfield>DEVICEID</idfield>
	</clickfunction>
	
	<popup>
		<title>{DEVICEID}</title>
		<fields>
			<field name="DEVICEID" />
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<popupskinclass></popupskinclass>
	
	<simplerenderer>
		<picturemarkersymbol url="assets/images/mapIcons/TianJin/SheXiangJi.png"/>
	</simplerenderer>
</configuration>