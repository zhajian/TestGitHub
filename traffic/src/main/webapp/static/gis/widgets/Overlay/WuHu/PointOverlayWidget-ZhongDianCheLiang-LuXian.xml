<!-- 交保专题的重点车辆 -->
<?xml version="1.0" ?>
<configuration>
	<type>ZhongDianCheLiang</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<initvisible>true</initvisible>
	
	<popup>
		<title>{hphm}</title>
		<fields>
			<field name="hphm"/>
			<field name="jgsj" alias="经过时间：" visible="true"/>
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<simplerenderer>
		<picturemarkersymbol url="assets/images/mapIcons/WuHu/JingChe-red.png" yoffset="18" />
	</simplerenderer>
	
	<clickfunction>
		<action>showInfoWindow</action>
	</clickfunction>
</configuration>
