<?xml version="1.0" ?>
<configuration>
	<type>odLine</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<simplerenderer>
		<simplelinesymbol style="solid" color="0x0055BF" alpha="1" width="3"/>
	</simplerenderer>
	
	<arrow enable="true">
		<position>middle</position>
		<length>20</length>
		<simplerenderer>
			<gradientfill linecolor="0x000000" linewidth="1">
					<gradiententry color="0x006bcf" alpha="0.8"/>
					<gradiententry color="0x0084ff" alpha="0.8"/>
				</gradientfill>
		</simplerenderer>
	</arrow>
	
