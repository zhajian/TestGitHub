<?xml version="1.0" ?>
<configuration>
	<type>travelTimeLine</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<simplerenderer>
		<simplelinesymbol style="solid" color="0x0055BF" alpha="1" width="3"/>
	</simplerenderer>
	
	<!-- 发光效果 -->
	<glowfilter>
		<alpha>1</alpha>
		<blurx>4</blurx>
		<blury>4</blury>
		<strength>80</strength>
		<color>0xFFFFFF</color>
		<!-- 内发光 -->
		<inner>false</inner>
		<!-- 镂空 -->
		<knockout>false</knockout>
	</glowfilter>
</configuration>