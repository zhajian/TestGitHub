<?xml version="1.0" ?>
<configuration>
	<type>addressPoint</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<popup>
		<title>{name}</title>
		<fields>
			<field name="name"/>
			<field name="age" alias="年龄" visible="true"/>
			<field name="sex" alias="性别" visible="true"/>
			<field name="card" alias="证件号" visible="true"/>
			<field name="addr" alias="地址" visible="true"/>
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<simplerenderer>
		<picturemarkersymbol url="assets/images/building/fbsxq.png"/>
	</simplerenderer>
</configuration>
