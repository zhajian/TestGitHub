<?xml version="1.0" ?>
<configuration>
	<type>school-park</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify>onSchoolParkOverlayRun</widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify>onSchoolParkOverlayClose</widgetclosenotify>
	<popup>
		<title>{name}</title>
		<fields>
			<field name="name" visible="false"/>
			<field name="id" alias="线路" visible="true"/>
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
		<buttons>
			<button label="详情" action="openDevice" type="schoolPark" />
		</buttons>
	</popup>
	<simplerenderer>
		<picturemarkersymbol url="assets/images/mapIcons/TianJin/XiaoChe/XueXiao.png" yoffset="13" />
	</simplerenderer>
</configuration>
