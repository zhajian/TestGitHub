<?xml version="1.0" ?>
<configuration>
	<type>busRoute</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify>onBusRouteOverlayRun</widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify>onBusRouteOverlayClose</widgetclosenotify>
	
	<uniquevaluerenderer field="lineNo">
		<simplelinesymbol style="solid" color="0x000000" alpha="1" width="3"/>
		
		<uniquevalueinfo value="生态城公交1号线">
			<simplelinesymbol style="solid" color="0xfc5068" alpha="1" width="3"/>
		</uniquevalueinfo>
		
		<uniquevalueinfo value="生态城公交2号线">
			<simplelinesymbol style="solid" color="0x538a4e" alpha="1" width="3"/>
		</uniquevalueinfo>
		
		<uniquevalueinfo value="生态城公交3号线">
			<simplelinesymbol style="solid" color="0x526afb" alpha="1" width="3"/>
		</uniquevalueinfo>
		
		<uniquevalueinfo value="生态城公交4号线">
			<simplelinesymbol style="solid" color="0xd3d451" alpha="1" width="3"/>
		</uniquevalueinfo>
	</uniquevaluerenderer>
</configuration>