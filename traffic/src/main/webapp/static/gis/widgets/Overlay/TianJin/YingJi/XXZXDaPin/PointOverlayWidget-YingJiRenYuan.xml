<?xml version="1.0" ?>
<!-- 应急人员 -->
<configuration>
	<type>YingJiRenYuan</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify></widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<needzoom>false</needzoom>
	<zoomscale>48000</zoomscale>
	
	<initvisible>true</initvisible>
	
	
	<popupskinclass>天津信息中心大屏应急人员</popupskinclass>
	
	<popup>

		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<uniquevaluerenderer field="online">
		<picturemarkersymbol url="assets/images/mapIcons/WuHu/JingCha-grey.png" yoffset="18"/>
		
		<uniquevalueinfo value="在线">
			<picturemarkersymbol url="assets/images/mapIcons/WuHu/JingCha-blue.png" yoffset="18"/>
		</uniquevalueinfo>
		
		<uniquevalueinfo value="离线">
			<picturemarkersymbol url="assets/images/mapIcons/WuHu/JingCha-yellow.png" yoffset="18"/>
		</uniquevalueinfo>
	</uniquevaluerenderer>
	
	<clickfunction>
		<action>showInfoWindow</action>
	</clickfunction>
</configuration>
