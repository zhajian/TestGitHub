<?xml version="1.0" ?>
<configuration>
	<type>event</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify>onEventOverlayRun</widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify></widgetclosenotify>
	
	<needzoom>true</needzoom>
	<zoomscale>48000</zoomscale>
	
	<initvisible>false</initvisible>
	
	
	<popupskinclass>TianJinShiJian</popupskinclass>
	
	<popup>
		<showzoomtobutton>false</showzoomtobutton>
	</popup>
	
	<simplerenderer>
		<picturemarkersymbol url="assets/images/mapIcons/TianJin/JiaoTongYingJiShiJian.png"/>
	</simplerenderer>
	
	<clickfunction>
		<action>showInfoWindow</action>
	</clickfunction>
</configuration>
