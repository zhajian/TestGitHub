<!-- 应急单位 -->
<?xml version="1.0" ?>
<configuration>
	<type>YingJiDanWei</type>
	<!-- 组件打开时通知页面 -->
	<widgetrunnotify>onYingJiDanWeiOverlayRun</widgetrunnotify>
	<!-- 组件关闭时通知页面 -->
	<widgetclosenotify>onYingJiDanWeiOverlayClose</widgetclosenotify>
	
	<initvisible>false</initvisible>
	
	<popupskinclass>advanced</popupskinclass>
	<popup>
		<title>{dwmc}</title>
		<fields>
			<field name="dwmc"/>
			<field name="dwqc" alias="单位全称：" visible="true"/>
			<field name="dz" alias="地址：" visible="true"/>
			<field name="yb" alias="邮编：" visible="true"/>
			<field name="cz" alias="传真：" visible="true"/>
		</fields>
		<showzoomtobutton>false</showzoomtobutton>
<!-- 		<buttons> -->
<!-- 			<button label="拨号" action="openDevice" type="callPoliceCarPDA"/> -->
<!-- 		</buttons> -->
	</popup>
	
	<simplerenderer>
		<picturemarkersymbol url="assets/images/mapIcons/WuHu/JingCha-grey.png" yoffset="18" />
	</simplerenderer>
	
	<clickfunction>
		<action>showInfoWindow</action>
	</clickfunction>
</configuration>
