<configuration>
	<type>point</type>
	
	<!-- point/polyline/freeHandPolyline/polygon/freeHandPolygon/circle/ellipse -->
	<drawtype>point</drawtype>
	<drawsymbol>
		<simplemarkersymbol color="0x3E6FBC" style="circle" size="10"/>
	</drawsymbol>
	
	<showbuffer>false</showbuffer>
	<defaultbufferdistance>200</defaultbufferdistance>
	
	<showconfirmwindow>true</showconfirmwindow>
	<confirmwindow>
		<title>添加点位</title>
		<text>是否在此位置添加点位？</text>
	</confirmwindow>
</configuration>