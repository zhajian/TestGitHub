<?xml version="1.0" ?>
<!-- 设施病害菜单 -->
<configuration>
	<menu>
		<itemgroup label="业务图层">
			<item label="设施病害" initenable="true">
				<clickfunctions>
					<clickfunction name="ChangeOverlayVisible">
						<params>
							<param name="WidgetName" value="设施病害" />
							<param name="OverlayType" value="FaultDevice" />
						</params>
					</clickfunction>
				</clickfunctions>
			</item>
		</itemgroup>
		
		<itemgroup label="设备图层">
			<item label="摄像机">
				<clickfunctions>
					<clickfunction name="ChangeWidget">
						<params>
							<param name="WidgetName" value="摄像机" />
						</params>
					</clickfunction>
				</clickfunctions>
			</item>
		</itemgroup>
		
		<itemgroup label="功能">
			<item label="测距">
				<clickfunctions>
					<clickfunction name="ChangeWidget">
						<params>
							<param name="WidgetName" value="测距" />
						</params>
					</clickfunction>
				</clickfunctions>
			</item>
			
			<item label="搜索">
				<clickfunctions>
					<clickfunction name="ChangeWidget">
						<params>
							<param name="WidgetName" value="搜索" />
						</params>
					</clickfunction>
				</clickfunctions>
			</item>
		</itemgroup>
	</menu>
</configuration>